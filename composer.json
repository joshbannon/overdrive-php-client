{
  "name": "joshbannon/OverDrivePHPClient",
  "description": "An API client for overdrive.com to query library and patron information, place checkouts and holds, check availability, and download titles.",
  "license": "MIT",
  "authors": [
    {
      "name": "Joshua Bannon",
      "email": "jdbannon+git@gmail.com",
      "homepage": "https://github.com/joshbannon"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "guzzlehttp/guzzle": "6.0.1",
    "alekseykorzun/memcached-wrapper-php": "v2.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.0"
  },
  "autoload": {
    "psr-4": {
      "OverDrivePHPClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OverDrivePHPClient\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.8-dev"
    }
  }
}
